#include <stdio.h>

int main() {
    int vetor[5];
    int i, soma = 0;
    
    printf("Digite cinco numeros inteiros:\n");
    for(i = 0; i < 5; i++) {
        printf("Numero %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }


    for(i = 0; i < 5; i++) {
        soma += vetor[i];
    }


    printf("A soma dos numeros digitados e: %d\n", soma);

    return 0;
}
